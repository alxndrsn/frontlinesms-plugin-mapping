package com.ushahidi.plugins.mapping.data.domain;

public class ReportIncident {
	private int id;
	private String incidentTitle;
	private String incidentDescription;
	private boolean markedForPosting;
	
	public void setId(int id){
		this.id  = id;
	}
	
	public int getId(){
		return id;
	}
	
	public void setIncidentTitle(String title){
		this.incidentTitle = title;
	}
	
	public String getIncidentTitle(){
		return incidentTitle;
	}
	
	public void setIncidentDescription(String description){
		this.incidentDescription = description;
	}
	
	public String getIncidentDescription(){
		return incidentDescription;
	}
	
	public void markForPosting(boolean value){
		this.markedForPosting = value;
	}
	
	public boolean isMarkedForPosting(){
		return markedForPosting;
	}
}
