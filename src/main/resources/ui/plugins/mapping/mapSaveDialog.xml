<?xml version="1.0" encoding="ISO-8859-1"?>
<dialog closable="true" close="removeDialog(this)" modal="true" bottom="5" columns="3" gap="5" icon="/icons/export.png" left="5" name="exportDialog" right="5" text="i18n.common.map.save" top="5">
    <label text="i18n.common.map.save.name"/>
    <textfield columns="40" name="tfMapName" perform="doMapSave(exportDialog, tfMapName.text)" weightx="2"/>
    <separator colspan="3"/>
    <separator rowspan="0"/>
    <button action="doMapSave(exportDialog, tfMapName.text)" halign="right" icon="/icons/tick.png" name="btDone" text="i18n.action.save"/>
    <button action="removeDialog(exportDialog)" icon="/icons/cross.png" text="i18n.action.cancel"/>
</dialog>
