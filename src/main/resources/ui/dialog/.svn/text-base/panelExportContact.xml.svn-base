<?xml version="1.0" encoding="ISO-8859-1"?>
<panel bottom="5" columns="2" gap="5" left="5" name="pnContactInfo" right="5" top="5" weightx="1" weighty="1">
    <label alignment="center" colspan="2" font="bold" i18n="true" text="i18n.sentence.what.do.you.want.to.export.from.contacts"/>
    <checkbox icon="/icons/user.png" name="cbName" selected="true" text="i18n.common.name"/>
    <checkbox icon="/icons/phone_number.png" name="cbPhone" selected="true" text="i18n.common.phone.number"/>
    <checkbox icon="/icons/phone_number.png" name="cbOtherPhone" selected="true" text="i18n.common.other.phone.number"/>
    <checkbox icon="/icons/email.png" name="cbEmail" selected="true" text="i18n.common.email.address"/>
    <checkbox icon="/icons/user_active.png" name="cbStatus" selected="true" text="i18n.common.current.status"/>
    <checkbox icon="/icons/note.png" name="cbNotes" selected="true" text="i18n.common.notes"/>
    <checkbox colspan="2" icon="/icons/group.png" name="cbGroups" selected="true" text="i18n.common.groups"/>
    <label rowspan="13"/>
    <panel bottom="3" colspan="2" gap="5" top="3">
        <label text="i18n.common.filename"/>
        <textfield columns="26" name="tfFilename" weightx="1"/>
        <button action="showSaveModeFileChooser(tfFilename)" icon="/icons/browse.png" text="i18n.action.browse" weightx="1"/>
    </panel>
    <panel colspan="2" gap="5" halign="center">
        <button action="doExport" halign="right" icon="/icons/export.png" text="i18n.common.export"/>
    	<button action="removeWizard" halign="left" icon="/icons/cross.png" text="i18n.action.cancel"/>
    </panel>
</panel>
